        <activity
            android:name=".RelativeLayoutDemoActivity"
            android:exported="false" />
        <activity
            android:name=".LinearLayoutDemoActivity"
            android:exported="false" />
        <activity
            android:name=".FrameLayoutActivity"
            android:exported="false" />
        <activity
            android:name=".ConstraintLayoutDemoActivity"
            android:exported="false" />
        <activity
            android:name=".SecondLayoutDemo"
            android:exported="false" />
        <activity
            android:name=".NotificationActivity"
            android:exported="false" />
        <activity
            android:name=".AlertDialogActivity"
            android:exported="false" />
        <activity
            android:name=".FragmentTaskActivity"
            android:exported="false" />
        <activity
            android:name=".RecycleViewDemo"
            android:exported="false" />
        <activity
            android:name=".TabLayoutDemo"
            android:exported="false" />
        <activity
            android:name=".VectorShapeSelector"
            android:exported="false" />
        <activity
            android:name=".CustomButtonWithprogressbar"
            android:exported="false" />
        <activity
            android:name=".Profile"
            android:exported="false" />
        <activity
            android:name=".Registration"
            android:exported="false" />
        <activity
            android:name=".DrawableDemo"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <provider
            android:authorities="com.example.demoapp.provider"
            android:name="androidx.core.content.FileProvider"
                android:exported="false"
            android:grantUriPermissions="true"
            >
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/paths"
                />
        </provider>
    </application>

</manifest>