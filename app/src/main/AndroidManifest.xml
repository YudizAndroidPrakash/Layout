            android:exported="false" />
        <activity
            android:name=".BroadCastReceiverSecondActivity"
            android:exported="false" />

        <receiver
            android:name=".MyBroadCastReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="android.intent.action.BATTERY_CHANGED" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".BroadCastReceiverOne"
            android:exported="false" />
